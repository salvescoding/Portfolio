<div class="inner cover">
  <h3 class="cover-heading">Coding Tweets</h3>
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser(tweet) %>
          </div>
        </div>
      </div>
    <% end  %>
  </div>

</div>


